{
    "name": "diablomedia/zendframework1-service-recaptcha",
    "description": "Zend Framework 1 Service_Recaptcha component",
    "type": "library",
    "keywords": [
        "framework",
        "zf1"
    ],
    "homepage": "http://framework.zend.com/",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ~8.0.0 || ~8.1.0 || ~8.2.0",
        "diablomedia/zendframework1-config": "dev-php-82 as 2.0.0",
        "diablomedia/zendframework1-json": "dev-php-82 as 1.0.0",
        "diablomedia/zendframework1-http": "dev-php-82 as 1.0.0",
        "diablomedia/zendframework1-service": "dev-php-82 as 1.0.0",
        "diablomedia/zendframework1-validate": "dev-php-82 as 1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Zend_Service_ReCaptcha_": "src/"
        },
        "classmap": [
            "src/Zend/Service/ReCaptcha.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Zend_": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "no-api": true,
            "url": "https://github.com/diablomedia/phpstorm-stubs"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "phpstan/phpstan": "1.10.13",
        "friendsofphp/php-cs-fixer": "3.16.0",
        "maglnet/composer-require-checker": "^3.0.0",
        "phpro/grumphp-shim": "^1.5.0"
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test-with-coverage": "phpunit --coverage-clover=clover.xml",
        "phpstan": "phpstan analyze -c phpstan.neon --no-progress --ansi",
        "style-check": "php-cs-fixer fix --dry-run -vv"
    },
    "suggest": {
        "ext-mcrypt": "Used in the MailHide class"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
